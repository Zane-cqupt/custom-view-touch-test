<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyImageButton">
        <attr name="Imagesrc" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DoubleImageView">
        <attr name="drawableLeft" format="reference"/>
        <attr name="drawableRight" format="reference"/>
        <attr name="text2" format="string"/>
        <attr name="textSize2" format="dimension"/>
        <attr name="textColor2" format="color"/>
        <attr name="spacing2" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="BoxGridLayout">
        <attr name="splitWidth" format="dimension"/>
        <attr name="splitColor" format="color"/>
        <attr name="columnsNum" format="integer"/>
    </declare-styleable>

    <declare-styleable name="FigureImageView">
        <attr name="mode" format="enum">
            <enum name="circle" value="0"/>
            <enum name="round_rect" value="1"/>
            <enum name="sector" value="2"/>
            <enum name="ring" value="3"/>
        </attr>
        <attr name="radius" format="dimension"/>
        <attr name="angle" format="float"/>
        <attr name="length" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PieChart">
        <attr name="name" format="string"/>
        <attr name="percentDecimal" format="integer"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
</resources>